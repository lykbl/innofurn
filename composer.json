{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/socialite": "^5.11",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "lunarphp/lunar": "^0.7.1",
        "mll-lab/graphql-php-scalars": "^6.2",
        "nuwave/lighthouse": "^6.22",
        "pusher/pusher-php-server": "^7.2"
    },
    "require-dev": {
        "cweagans/composer-patches": "^1.7",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.26",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mll-lab/laravel-graphiql": "^3.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0",
        "symplify/vendor-patches": "^11.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "patches": {
            "intervention/image": [
                "patches/intervention-image-src-intervention-image-gd-decoder-php.patch"
            ],
            "lunarphp/lunar": [
                "patches/lunarphp-lunar-packages-core-database-state-converttaxbreakdown-php.patch"
            ]
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "cweagans/composer-patches": true
        },
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "post-install-cmd": [
        "make post-deploy"
    ]
}
